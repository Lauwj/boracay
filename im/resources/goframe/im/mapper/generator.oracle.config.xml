<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration >
    <classPathEntry location="../lib/jdbc/ojdbc6.jar" />

    <context id="context1"  targetRuntime="MyBatis3Simple"><!--&lt;!&ndash;  targetRuntime="MyBatis3Simple" &ndash;&gt;-->
        <commentGenerator>
            <property name="suppressAllComments" value="true" />
            <property name="suppressDate" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="oracle.jdbc.OracleDriver" connectionURL="jdbc:oracle:thin:@192.168.1.61:1521/orcl" userId="udsp" password="udsp" />
        <javaTypeResolver >
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <javaModelGenerator targetPackage="com.hex.bigdata.udsp.im.model" targetProject="src">
            <property name="enableSubPackages" value="true" />
            <property name="trimStrings" value="false" />
        </javaModelGenerator>
        <sqlMapGenerator targetPackage="goframe.im.mapper.general" targetProject="resources">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>
        <javaClientGenerator targetPackage="com.hex.bigdata.udsp.im.dao" targetProject="src" type="XMLMAPPER">
            <property name="enableSubPackages" value="true" />
        </javaClientGenerator>

        <table tableName="IM_METADATA" domainObjectName="ImMetadata"/>
        <table tableName="IM_METADATA_COLUMN" domainObjectName="ImMetadataCol"/>
        <table tableName="IM_MODEL" domainObjectName="ImModel"/>
        <table tableName="IM_MODEL_MAPPING" domainObjectName="ImModelMapping"/>
        <table tableName="IM_MODEL_FILTER_COLUMN" domainObjectName="ImModelFilterCol"/>
  </context>
</generatorConfiguration>